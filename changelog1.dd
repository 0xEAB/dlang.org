$(VERSION 126, Jun 7, 2005, ===================================================,

$(WHATSNEW
        $(LI $(B ===) now deprecated, replaced with $(B is).)
        $(LI $(B !==) now deprecated, replaced with $(B !is).)
        $(LI Added <a href="function.html#variadic">typesafe variadic functions</a>.)
        $(LI Now throws an ArrayBoundsError if accessing an associative
        array with a key that is not already in the array. Previously,
        the key would be added to the array.)
        $(LI Added $(ISEXPRESSIONS)s
        which replaces $(I IftypeCondition)s
        which are now deprecated.
        The $(ISEXPRESSION)s are significantly
        more general and powerful.)
        $(LI Added <a href="memory.html#uninitializedarrays">$(I VoidInitializers)</a>.)
        $(LI $(TT delete aa[key]) is now deprecated, use
        $(TT aa.remove(key)) instead.)
        $(LI Added <a href="class.html#nested">non-static nested classes</a>.)
        $(LI Added <a href="class.html#anonymous">anonymous non-static nested
        classes</a>.)
        $(LI Incorporated Ben Hinkle's changes to std.stream:
        $(UL
            $(LI std.stream.stdin/out/err deprecated)
            $(LI adding std.cstream.din/out/err)
            $(LI stream.d:
            $(UL
                $(LI  incorporate the socketstream code for handling \r
                end-of-line formats for readLine on non-seekable streams like
                pipes)
                $(LI  getc and getcw return char.init and wchar.init on EOF)
                $(LI  getcw throws if EOF after reading 1 byte)
                $(LI  ungetc and ungetcw ignore char.init and wchar.init)
                $(LI  deprecate stdin,stdout,stderr)
                $(LI  remove private import of std.c.stdio and aliases for
                va_list)
            )
            )
            $(LI socketstream.d: removed overrides that are now in std.stream.Stream)
            $(LI cstream.d: new stream wrapper around FILE* with din, dout, derr)
            $(LI unittest,d: add cstream to ensure unittests are run)
        )
        )
)

