Macros:
        TITLE=Change Log
        WIKI=ChangeLog

        NEW = $(LI What's new for <a href="#new0$0">D 0.$0</a>)

        VERSION=
        <div id=version>
        <h3><a name="new0$1">
          What's New for
          <a href="http://ftp.digitalmars.com/dmd.$1.zip" title="D 0.$1">D 0.$1</a>
          </a>
        </h3>
        $(SMALL $(I $2, $3))
        $5
        </div>

        VERSIONN=
        <div id=version>
        <h3><a name="new0$1">What's New for D 0.$1</a></h3>
        $(SMALL $(I $2, $3))
        $5
        </div>

        BUGZILLA = <a href="http://d.puremagic.com/issues/show_bug.cgi?id=$0">Bugzilla $0</a>

        BUGSFIXED = <div id="bugsfixed"><h4>Bugs Fixed</h4> $(UL $0 )</div>
        WHATSNEW = <div id="whatsnew"><h4>New/Changed Features</h4> $(UL $0 )</div>
        LARGE=<font size=4>$0</font>

